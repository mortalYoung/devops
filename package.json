{
  "name": "dtstack-devops",
  "displayName": "dtstack-devops",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.x"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "dtstack-devops.refresh",
          "when": "view == dtstack.devops",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "dtstack-devops.onCreateDev",
          "when": "viewItem == dev"
        },
        {
          "command": "dtstack-devops.onCreateMR",
          "when": "viewItem == dev"
        },
        {
          "command": "dtstack-devops.onCreateDevBus",
          "when": "viewItem == test"
        },
        {
          "command": "dtstack-devops.onCreateFix",
          "when": "viewItem == test"
        },
        {
          "command": "dtstack-devops.onCreateTest",
          "when": "viewItem == release"
        },
        {
          "command": "dtstack-devops.onCreateHotfix",
          "when": "viewItem == release"
        },
        {
          "command": "dtstack-devops.onCreateIterator",
          "when": "viewItem == project"
        }
      ]
    },
    "commands": [
      {
        "command": "dtstack-devops.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/refresh.svg",
          "dark": "resources/refresh.svg"
        }
      },
      {
        "command": "dtstack-devops.onCreateDev",
        "title": "新建开发分支"
      },
      {
        "command": "dtstack-devops.onCreateMR",
        "title": "创建 MR"
      },
      {
        "command": "dtstack-devops.onCreateDevBus",
        "title": "新建开发总分支"
      },
      {
        "command": "dtstack-devops.onCreateFix",
        "title": "新建修复分支"
      },
      {
        "command": "dtstack-devops.onCreateTest",
        "title": "新建提测分支"
      },
      {
        "command": "dtstack-devops.onCreateHotfix",
        "title": "新建 hotfix 分支"
      },
      {
        "command": "dtstack-devops.onCreateIterator",
        "title": "新建迭代"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dtstack_devops",
          "title": "Devops",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "dtstack_devops": [
        {
          "id": "dtstack.devops",
          "name": "Devops",
          "when": "true"
        }
      ]
    },
    "configuration": {
      "title": "dtstack_devops",
      "properties": {
        "dtstack_devops.gitlabUrl": {
          "type": "string",
          "default": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "simple-git": "^3.19.0"
  }
}